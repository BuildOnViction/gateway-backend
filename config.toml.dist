[log]
format = "json"
level = "info"

[telemetry]
addr = ":10000"

[opencensus.exporter]
enabled = false
address = "127.0.0.1:55678"
insecure = false
reconnectPeriod = "5s"

[opencensus.trace]
sampling = { sampler = "always" }
# sampling = { sampler = "probability", fraction = 0.5 }

[opencensus.prometheus]
enabled = false

[app]
httpAddr = ":8000"
grpcAddr = ":8001"

storage = "inmemory"

[database]
uri = "mongodb://tomobridgejob:anhlavip@localhost:27017/tomobridgejob"
